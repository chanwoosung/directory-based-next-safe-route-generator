{
  "name": "next-safe-router",
  "version": "1.0.1",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/runtime/safe-router.js",
      "types": "./dist/runtime/safe-router.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:ts": "tsup src/index.ts src/runtime/safe-router.ts --dts --format esm,cjs --out-dir dist --external next",
    "build": "npm run build:ts"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "ts-node": "^10.9.2",
    "tsup": "8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "next": ">=14"
  }
}